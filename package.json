{
  "name": "webpack-es6-sass-setup",
  "version": "0.0.1",
  "description": "A sample setup for Webpack with ES6 and Sass",
  "main": "webpack.config.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --config webpack.config.js --port 3001 --hot --inline",
    "test": "mocha --require babel-core/register ./src/test/**/*.js",
    "test:watch": "mocha --require babel-core/register --watch --watch-extensions js ./src/test/**/*.js",
    "bundle": "./node_modules/.bin/webpack --config webpack.config.js"
  },
  "author": "Aki Karkkainen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/akikoo/webpack-es6-sass-setup.git"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.7",
    "es6-promise": "^4.1.1",
    "eslint": "^4.12.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "expect": "^21.2.1",
    "jsdom": "^11.5.1",
    "mocha": "^4.0.1",
    "mocha-jsdom": "^1.1.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.8",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-selector-bem-pattern": "^2.0.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "keywords": [
    "babel",
    "boilerplate",
    "es6",
    "sass",
    "starter-kit",
    "workflow",
    "webpack"
  ],
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "event-pubsub": "^4.3.0",
    "extract-text-webpack-plugin": "^3.0.1"
  }
}
